<%- include('../includes/head.ejs') %>
</head>
<body>
<%- include('../includes/nav.ejs') %>
<main>
	<% if(orders.length <= 0) { %>
	<h1>Nothing here!</h1>
	<% } else { %>
		<ul>
			<% orders.forEach(order => { %>
				<li>
					<h1># <%= order._id %></h1>
					<ul>
						<% order.products.forEach(p => { %>
						<li><%= p.product.title %> (<%= p.quantity %>)</li>
						<% }); %>
						<li>Total Price: <%= order.price %></li>
					</ul>
				</li>
			<% }); %>
		</ul>
	<% } %>
</main>
<%- include('../includes/end.ejs') %>